{
  "name": "ebpf-snippets",
  "displayName": "eBPF Snippets",
  "description": "Code-snippets for developing eBPF programs",
  "repository": {
    "type": "git",
    "url": "https://github.com/h0x0er/ebpf-snippets"
  },
  "author": {
    "name": "h0x0er",
    "url": "https://h0x0er.github.io/"
  },
  "version": "0.3.0",
  "engines": {
    "vscode": "^1.0.0"
  },
  "publisher": "h0x0er",
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "c",
        "path": "./snippets/maps.code-snippets"
      },
      {
        "language": "c",
        "path": "./snippets/progs-cgroup.code-snippets"
      },
      {
        "language": "c",
        "path": "./snippets/progs-tracing.code-snippets"
      },
      {
        "language": "c",
        "path": "./snippets/progs-network.code-snippets"
      },
      {
        "language": "c",
        "path": "./snippets/misc.code-snippets"
      }
    ]
  },
  "scripts": {
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.6.0",
    "add": "^2.0.6",
    "or": "^0.2.0",
    "yarn": "^1.22.22"
  }
}